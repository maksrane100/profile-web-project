ss=\"room\">\n\t\t\t\t\t\t\t\t\t\t<div class=\"room_num\">{{room.room_number}}:{{room.room_price}}</div>\n\t\t\t\t\t\t\t\t\t\t<div class=\"room_tenant_name\">{{room.tenant == null ? \"\" : room.tenant.tenant_name}}</div>\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t</el-card>\n\t\t\t\t\t\t\t</el-tooltip>\n\t\t\t\t\t\t</el-col>\n\t\t\t\t\t</template>\n\t\t\t\t</el-table-column>\n\t\t\t\t<el-table-column label=\"Operation\" width=\"119px\" class=\"header\">\n\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t<el-tooltip class=\"item\" effect=\"dark\" content=\"Increase room\" placement=\"top-start\" :enterable=\"false\">\n\t\t\t\t\t\t\t<el-button class=\"el-icon-plus\" type=\"primary\" @click.prevent=\"openAddRoom(scope.row)\" size=\"mini\"></el-button>\n\t\t\t\t\t\t</el-tooltip>\n\t\t\t\t\t\t<el-tooltip class=\"item\" effect=\"dark\" content=\"Delete house\" placement=\"top-start\" :enterable=\"false\">\n\t\t\t\t\t\t\t<el-button class=\"el-icon-delete\" type=\"danger\" @click.prevent=\"delHouse(scope.row)\" size=\"mini\"></el-button>\n\t\t\t\t\t\t</el-tooltip>\n\t\t\t\t\t</template>\n\t\t\t\t</el-table-column>\n\t\t\t</el-table>\n\t\t\t<el-pagination @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\" :current-page=\"page\"\n\t\t\t :page-sizes=\"[1, 2, 5, 10, 20, 30, 40]\" :page-size=\"pagesize\" layout=\"total, sizes, prev, pager, next, jumper\" :total=\"searchHouse().length\">\n\t\t\t</el-pagination>\n\t\t</div>\n\n\t\t<el-dialog title=\"New House Information\" :visible.sync=\"dialogAddHouse\" width=\"45%\" :before-close=\"handleClose\" :showClose=\"showClose\">\n\t\t\t<el-form :model=\"houseInfo\" status-icon :rules=\"rules\" label-width=\"130px\" ref=\"addhouseInfoFrom\">\n\t\t\t\t<el-form-item label=\"Community\" prop=\"house_region\">\n\t\t\t\t\t<el-input type=\"text\" v-model=\"houseInfo.house_region\" autocomplete=\"off\" placeholder=\"Please enter a community name\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t\t<el-form-item label=\"D